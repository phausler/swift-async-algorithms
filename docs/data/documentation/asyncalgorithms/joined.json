            },
            {
              "identifier" : "https:\/\/github.com\/apple\/swift-async-algorithms\/blob\/main\/Tests\/AsyncAlgorithmsTests\/TestJoin.swift",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " ",
